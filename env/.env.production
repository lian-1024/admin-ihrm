# 是否开启 sourcemap (生产环境关闭以减小包体积)
IHRM_SOURCEMAP=false

# 是否开启 css 代码分割 (生产环境开启以优化加载性能)
IHRM_CSS_CODE_SPLIT=true

# 压缩配置 (生产环境开启压缩以减小包体积)
IHRM_MINIFY=true

# 是否开启 console (生产环境关闭以减小包体积和避免信息泄露)
IHRM_CONSOLE=false

# 是否开启 tree-shaking (生产环境开启以优化包体积)
IHRM_TREE_SHAKING=true


# 压缩配置
IHRM_MINIFY=true

# 是否开启产物分析报告
IHRM_BUILD_ANALYZE=true