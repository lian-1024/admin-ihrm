{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./env/env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/shared/components/*"
      ],
      "@composables/*": [
        "./src/shared/composables/*"
      ],
      "@core/*": [
        "./src/core/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@utils/*": [
        "./src/shared/utils/*"
      ],
      "@stores/*": [
        "./src/core/stores/*"
      ],
      "@constants/*": [
        "./src/shared/constants/*"
      ]
    }
  }
}